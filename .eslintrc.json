{
  "env": {
    "browser": true,
    "es2021": true
  },
  "extends": [
    "plugin:react/recommended",
    "prettier"
  ],
  "parser": "babel-eslint",
  "parserOptions": {
    "ecmaFeatures": {
      "jsx": true
    },
    "ecmaVersion": 12,
    "sourceType": "module"
  },
  "plugins": [
    "react",
    "prettier"
  ],
  "rules": {
    "import/no-extraneous-dependencies": "off",
    "prettier/prettier": "error",
    "react/jsx-props-no-spreading": ["error", {"custom": "ignore"}],
    "react/forbid-prop-types": "off"
  }
}
